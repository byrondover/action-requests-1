{
  "name": "action-request",
  "version": "0.2.1",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "firebase:build": "yarn && ng build --aot --build-optimizer && yarn functions:build && yarn patch:ngsw",
    "firebase:build:production": "yarn && ng build --aot --build-optimizer --prod --service-worker && yarn functions:build && yarn patch:ngsw",
    "deploy": "yarn firebase:build && firebase deploy --project staging",
    "deploy:production": "yarn firebase:build:production && firebase deploy --project production",
    "functions:build": "cd functions && yarn firebase:build && cd ..",
    "analyze": "yarn && ng build --aot --stats-json && webpack-bundle-analyzer dist/stats.json",
    "patch:ngsw": "node ./tools/patch/ngsw-worker-patch.js"
  },
  "dependencies": {
    "@angular/animations": "^9.1.1",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "^9.1.1",
    "@angular/compiler": "^9.1.1",
    "@angular/core": "^9.1.1",
    "@angular/forms": "^9.1.1",
    "@angular/http": "^7.2.16",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "^9.1.1",
    "@angular/platform-browser-dynamic": "^9.1.1",
    "@angular/router": "^9.1.1",
    "@angular/service-worker": "^9.1.1",
    "@angular/fire": "^6.0.0",
    "core-js": "^3.6.5",
    "firebase": "^7.14.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "ngx-file-drop": "^9.0.0",
    "object-hash": "^2.0.3",
    "rxjs": "^6.5.5",
    "rxjs-compat": "6.5.5",
    "xlsx": "^0.15.6",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "^9.1.1",
    "@angular/compiler-cli": "^9.1.1",
    "@angular/language-service": "^9.1.1",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "@types/object-hash": "^1.3.1",
    "codelyzer": "^5.2.2",
    "firebase-admin": "^8.10.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "protractor": "^5.4.3",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}